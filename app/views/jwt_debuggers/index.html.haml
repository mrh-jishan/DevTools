= turbo_frame_tag :jwt_debuggers do
  = simple_form_for :jwt_debuggers, url: jwt_debuggers_path, data: { controller: "buttonless-form", remote: "true" }, method: "post" do |f|
    .grid.p-4{ data: { controller: "split-grid" }}
      .px-2{data: { controller: "jwt-editor", "jwt-editor-target" => "editor" }}
      .gutter-col{"data-gutter-col" => ""}
      .grid-item.px-2= render "jwt_payload"